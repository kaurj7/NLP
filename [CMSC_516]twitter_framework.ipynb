      "source": [
        "#function to extract the tweets\n",
        "def get_tweets(word):\n",
        "  tweet_list=[]\n",
        "  tweets = tweepy.Cursor(api.search, q = word, lang=\"en\", tweet_mode='extended').items(5)\n",
        "  for tweet in tweets:\n",
        "    tweet_list.append(tweet.full_text)\n",
        "  return tweet_list"
      ],
